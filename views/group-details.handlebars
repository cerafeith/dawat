<section class="container mt-2">
    <h1 class="bd-title" id="content">{{group.name}}</h1>

    <div class="my-4"></div>
    <h6 class="font-weight-bold">Users</h6>
    <ul class="list-group">
        {{#each group.users}}
        <li class="list-group-item d-flex justify-content-between align-items-center">
            {{this.username}}
        </li>
        {{/each}}
    </ul>
</section>